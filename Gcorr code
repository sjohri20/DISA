#See i have written a small code based on GCorr method


from math import *
s=raw_input()
N=len(s)
Cg=0

def num(stg):
    if stg=='G':
        return 1
    else:
        return -1

for k in range(1,N):
    Ck=0
    for j in range(1,N-k):
        
        Ck=Ck+num(s[j])*num(s[j+k])
        #print Ck
    Cg=Cg+(abs(Ck)+0.0)/(N-k)

print (Cg+0.0)/(N-1)

        
